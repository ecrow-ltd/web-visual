{
  "name": "@ecrow/visual-web",
  "library": "visual-web",
  "version": "1.0.0",
  "description": "Library of Visual Components for eCrow Ltd.",
  "private": true,
  "main": "./.lib/index.js",
  "types": "./.lib/index.d.ts",
  "files": [
    ".dist",
    ".lib",
    "src"
  ],
  "scripts": {
    "clean": "npm run clean:lib && npm run clean:dist",
    "clean:dist": "rimraf ./.dist/*",
    "clean:lib": "rimraf ./.lib/*",
    "build": "npm run build:lib && npm run build:dist",
    "build:dist": "npm run build:dist:dev && npm run build:dist:prod",
    "build:dist:dev": "webpack --dest distribution --env dev --config config/webpack.config.js",
    "build:dist:prod": "webpack --dest distribution --env prod --config config/webpack.config.js",
    "build:lib": "tsc --sourceRoot ./src --outDir ./.lib -d --declarationDir ./.lib",
    "build:storybook": "build-storybook -c ./config/storybook -s ./public -o .build",
    "build:doc": "jsdoc src -c ./jsdoc.config.json",
    "start": "npm run start:storybook",
    "start:lib": "nodemon --exec 'yarn build:lib' --watch ./src",
    "start:storybook": "start-storybook -p 2769 -s ./public -c ./config/storybook",
    "dev": "webpack --progress --colors --watch --env dev --config config/webpack.config.js",
    "repl": "node -i -e \"$(< ./lib/visual-web.js)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecrow-ltd/visual-web.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Eric Crowell",
  "license": "ISC",
  "homepage": "https://github.com/ecrow-ltd/visual-web#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "react-docgen-typescript-loader": "^3.1.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.12.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  }
}
