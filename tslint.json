{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "no-empty": false,
    "no-empty-interface": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "trailing-comma": true,
    "jsx-no-multiline-js": false,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "jsx-wrap-multiline": false,
    "quotemark": [true, "single", "jsx-double"]
  }
}
